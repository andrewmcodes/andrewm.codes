{
  "cleanUrls": true,
  "trailingSlash": true,
  "github": {
    "enabled": true
  },
  "redirects": [
    {
      "source": "/twitter/",
      "destination": "https://twitter.com/andrewmcodes"
    },
    {
      "source": "/source/",
      "destination": "https://github.com/andrewmcodes/andrewm.codes"
    },
    {
      "source": "/404/",
      "destination": "/404.html"
    },
    {
      "source": "/500/",
      "destination": "/500.html"
    },
    {
      "source": "/github/:path*",
      "destination": "https://github.com/andrewmcodes/:path*"
    },
    {
      "source": "/r/setapp/",
      "destination": "https://go.setapp.com/invite/3sztpuuq"
    },
    {
      "source": "/blog/webpacker-6-image-asset-guide/",
      "destination": "/webpacker-6/"
    },
    {
      "source": "/blog/webpacker-6-scss-sass-loaders/",
      "destination": "/webpacker-6/"
    },
    {
      "source": "/blog/webpacker-6-postcss-loaders/",
      "destination": "/webpacker-6/"
    },
    {
      "source": "/blog/webpacker-6-css-loaders/",
      "destination": "/webpacker-6/"
    },
    {
      "source": "/blog/webpacker-6-tailwind-css-2-0-integration/",
      "destination": "/webpacker-6/"
    },
    {
      "source": "/blog/webpacker-6-upgrade-guide/",
      "destination": "/webpacker-6/"
    },
    {
      "source": "/blog/webpacker-6-tutorial-setup/",
      "destination": "/webpacker-6/"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin"
        },
        {
          "key": "Permissions-Policy",
          "value": "geolocation=(self), microphone=()"
        }
      ]
    },
    {
      "source": "/feed.json",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/feed+json"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600"
        }
      ]
    },
    {
      "source": "/feed.xml",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/rss+xml"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600"
        }
      ]
    },
    {
      "source": "/sitemap.xml",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/xml"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600"
        }
      ]
    },
    {
      "source": "/robots.txt",
      "headers": [
        {
          "key": "Content-Type",
          "value": "text/plain"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600"
        }
      ]
    },
    {
      "source": "/_bridgetown/static/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
