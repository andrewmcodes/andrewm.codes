<%= render Container.new do %>
  <%= render List.new do %>
    <% site.data.raindrop.accounts.items.each do |account| %>
      <%= render ListTile.new(
            url: account.link,
            title: account.title,
            image: account&.cover,
            clamp: false) %>
    <% end %>
  <% end %>
<% end %>
